{
    "dependencies": {
        "lodashEs": "npm:lodash@4.17.21",
        "vue": "3.5.18"
    },
    "devDependencies": {
        "@playwright/test": "1.54.2",
        "@types/node": "24.2.0",
        "@vitejs/plugin-vue": "6.0.1",
        "@vitest/ui": "^3.2.4",
        "eslint": "9.32.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prettier": "5.5.3",
        "eslint-plugin-vue": "10.4.0",
        "prettier": "3.6.2",
        "vite": "7.0.6",
        "vitest": "3.2.4"
    },
    "name": "pivotable",
    "private": true,
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "format": "prettier .  --write",
        "lint": "eslint --fix src/main/resources/static",
        "lint_debug": "eslint --fix src/main/resources/static --debug",
        "preview": "vite preview",
        "unit_tests": "vitest",
        "unit_tests_ci": "vitest --reporter=html --outputFile=./vitest-report/test-output.html",
        "pw_localhost8080": "SPRING_ACTIVE_PROFILES=unsafe,inmemory,fakeuser npx playwright test localhost8080-queries --project=chromium",
        "pw_localhost8080_ui": "SPRING_ACTIVE_PROFILES=unsafe,inmemory,fakeuser npx playwright test localhost8080-queries --project=chromium --ui"
    },
    "type": "module",
    "version": "0.0.0"
}
